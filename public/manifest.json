{
    "manifest_version": 3,
    "name": "React Rails",
    "description": "Connect a React extension to a Rails API",
    "version": "1.0.0",
    "action": {
      "default_popup": "index.html",
      "default_title": "React Rails"
    },
    "permissions": [
      "scripting",
      "http://localhost:3000/*"
    ]
  }
  